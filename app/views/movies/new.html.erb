<div class="row">
  <div class="col s12 m4 l2 hidden-text">LUMOS</div>
  <div class="col s12 m4 l8">
    <%= form_for @movie, url: results_path, method: :get do |f| %>
      <div class="input-field">
        <%= f.text_field :title %>
        <%= f.label :title %>
      </div>

      <div class="input-field">
        <%= f.submit 'search', class: 'btn' %>
      </div>
    <% end %>
  </div>
  <div class="col s12 m4 l2 hidden-text">LUMOS</div>
</div>
