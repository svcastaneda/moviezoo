
<div class="col s4 m4">
  <div class="card">
    <div class="card-image">
      <img src="<%= movie.poster %>">

    </div>
    <div class="card-content">
      <span class="card-title">
        <a class="movie_title" href="<%= movie.id %>">
          <h5><%= movie.title %></h5>
        </a>
      </span>
      <p><%= movie.year %></p>
    </div>
  </div>
</div>

