<div class="center">
  <h2>Results</h2>
  <div class="row">
    <div class="col s12 m4 l2 hidden-text">LUMOS</div>
    <div class="col s12 m4 l8">

      <% @movies.each do |movie| %>
        <%= render partial: 'results/result', locals: {title: movie.title, year: movie.year, imdb_id: movie.imdb_id} %>
      <% end %>
    </div>
    <div class="col s12 m4 l2 hidden-text">LUMOS</div>
  </div>
</div>
